---
import NavigationLink from "./NavigationLink.astro";
const links = [
  { href: "/home", text: "Home" },
  { href: "/about/", text: "About" },
  { href: "/blog/", text: "Blog" },
  { href: "/tags/", text: "Tags" },
];
---

<nav class="flex">
  {links.map((link) => <NavigationLink href={link.href} text={link.text} />)}
</nav>
